{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
  {
    "matches": ["*://*/*"],
    "css": ["main.css"]
  }
  ],
  "description": "expand video in tab",
  "icons": {
    "240": "icon.png"
  },
  "manifest_version": 2,
  "name": "vid2tab",
  "permissions": [
    "menus",
    "activeTab"
  ],
  "version": "1.0.1"
}
